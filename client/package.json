{
  "name": "data-toolkit",
  "version": "0.0.5",
  "description": "A Quasar Project",
  "productName": "Data Toolkit",
  "author": "lizhongyue <lizhongyue248@163.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts,.vue ./",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "dev:electron:kernel": "cross-env RUN_SERVER=true quasar dev -m electron",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "generate:types": "npx openapi-typescript http://127.0.0.1:8080/openapi.json -o ./src/types/api.d.ts --enum -t",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:3001 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://127.0.0.1:3001 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@quasar/extras": "^1.16.11",
    "electron-log": "^5.1.4",
    "electron-store": "^9.0.0",
    "electron-updater": "^6.1.8",
    "find-process": "^1.4.7",
    "lodash": "^4.17.21",
    "openapi-fetch": "^0.9.5",
    "pinia": "^2.1.7",
    "quasar": "^2.16.2",
    "tree-kill": "^1.2.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@quasar/app-vite": "^2.0.0-beta.14",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.1.0",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vue/devtools": "^7.1.3",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.6.6",
    "electron": "^30.0.2",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.25.0",
    "openapi-typescript": "^6.7.5",
    "typescript": "~5.3.3",
    "vite-plugin-checker": "^0.6.4",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "electronDownload": {
    "mirror": "https://npm.taobao.org/mirrors/electron/"
  }
}
